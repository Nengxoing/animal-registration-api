{
  "name": "animal-registration-api",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --delay 400ms --exec ts-node src/index.ts",
    "start": "pm2-runtime dist/index.js",
    "build": "tsc && npm run copy-swagger",
    "copy-swagger": "npx copyfiles -u 1 src/swaggers/error-page.html src/swaggers/swagger-login.html dist",
    "createsuperuser": "ts-node src/api/user/createUserCLI.ts createsuperuser",
    "serve": "pm2-runtime start ecosystem.config.js",
    "start:prod": "node dist/index.js",
    "db": "npm run db:pull && npm run db:generate",
    "db:pull": "npx prisma db pull",
    "db:generate": "npx prisma generate",
    "db:migrate": "npx prisma migrate dev --name init",
    "db:migrate:deploy": "npx prisma migrate deploy",
    "db:migrate:reset": "npx prisma migrate reset --force",
    "db:studio": "npx prisma studio",
    "lint": "npx eslint \"src/**/*.ts\" \"src/api/**/*.ts\"",
    "lint:fix": "npx eslint \"src/**/*.ts\" \"src/api/**/*.ts\" --fix",
    "format": "npx prettier --write \"src/**/*.{ts,tsx,json,md}\" \"src/api/**/*.{ts,tsx,json,md}\"",
    "format:check": "npx prettier --check \"src/**/*.{ts,tsx,json,md}\" \"src/api/**/*.{ts,tsx,json,md}\"",
    "prepare": "husky install",
    "clean": "npx rimraf dist && npx rimraf prisma/dev.db && npx rimraf node_modules",
    "prisma:validate": "npx prisma validate",
    "prisma:format": "npx prisma format",
    "setup:env": "cp .env.example .env",
    "setup:db": "npm run db:migrate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "prisma": {
    "schema": "./src/prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^6.2.1",
    "bcrypt": "^5.1.1",
    "cls-hooked": "^4.2.2",
    "commander": "^13.1.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.0",
    "fs-extra": "^11.2.0",
    "http-status-codes": "^2.3.0",
    "inquirer": "^12.3.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodejs": "^0.0.0",
    "prom-client": "^15.1.3",
    "request-ip": "^3.3.0",
    "rotating-file-stream": "^3.2.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ua-parser-js": "^2.0.0",
    "uuid": "^11.0.3",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cls-hooked": "^4.3.9",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.0",
    "@types/express-rate-limit": "^6.0.2",
    "@types/fs-extra": "^11.0.4",
    "@types/http-status-codes": "^1.2.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.2",
    "@types/node-cron": "^3.0.11",
    "@types/readable-stream": "^4.0.18",
    "@types/request-ip": "^0.0.41",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.7",
    "@types/ua-parser-js": "^0.7.39",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.13",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.19.1",
    "concurrently": "^9.1.1",
    "eslint": "^9.17.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "prisma": "^6.2.1",
    "prisma-dbml-generator": "^0.12.0",
    "swagger-autogen": "^2.23.7",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.2"
  }
}
